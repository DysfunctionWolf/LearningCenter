<Window x:Class="LearningCenter.FrontEnd.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LearningCenter.FrontEnd"
        mc:Ignorable="d"
        Title="Estepp's Learning Center" Height="147.626" Width="337.291"
        ResizeMode="NoResize">
    <Grid >
        <Label Content="Employee ID:" Margin="10,10,0,0" HorizontalAlignment="Left" Width="86" Height="25" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox1" Margin="96,10,0,0" HorizontalAlignment="Left" Width="210" Height="25" VerticalAlignment="Top"/>
        <Label Content="Password:" Margin="10,40,0,0" HorizontalAlignment="Left" Width="86" Height="25" VerticalAlignment="Top"/>
        <PasswordBox x:Name="passwordBox1" PasswordChar="*" Margin="96,40,0,0" Height="25" VerticalAlignment="Top" HorizontalAlignment="Left" Width="210"/>
        <Button Margin="10,70,0,0" Content="Submit" HorizontalAlignment="Left" Width="86" Height="28" VerticalAlignment="Top" Click="Button_Click"/>
    </Grid>
</Window>
